Cell[CellGroupData[{

Cell["\<\
Rubel\[CloseCurlyQuote]s model, f(x), fits the given function y(x) in the \
interval (a,b) (exactly at the ends of the interval) :\
\>", "Subtitle",
 CellChangeTimes->{{3.5375485786875*^9, 3.53754863928125*^9}, {
  3.537548680640625*^9, 3.537548686734375*^9}, {3.53754909409375*^9, 
  3.537549122421875*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"f", "[", 
   RowBox[{"x_", ",", "a_", ",", "b_", ",", "ya_", ",", "yb_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    FractionBox[
     RowBox[{"yb", "-", "ya"}], "\[CapitalOmega]"], 
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      RowBox[{"-", "1"}], 
      FractionBox[
       RowBox[{
        RowBox[{"2", "x"}], "-", 
        RowBox[{"(", 
         RowBox[{"a", "+", "b"}], ")"}]}], 
       RowBox[{"b", "-", "a"}]]], 
     RowBox[{
      RowBox[{"Exp", "[", 
       RowBox[{
        RowBox[{"-", "1"}], "/", 
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"u", "^", " ", "2"}]}], ")"}]}], "]"}], 
      RowBox[{"\[DifferentialD]", "u"}]}]}]}], "+", "ya"}]}]], "Input",
