Cell[CellGroupData[{

Cell["\<\
Example of a continuous junction between two consecutive intervals, say (0,1) \
and (1,3/2) (numerical integration is needed) :\
\>", "Subtitle",
 CellChangeTimes->{{3.53754929725*^9, 3.53754936434375*^9}, {3.5375501655*^9, 
  3.537550178390625*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"fnum", "[", 
   RowBox[{"x_", ",", "a_", ",", "b_", ",", "ya_", ",", "yb_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    FractionBox[
     RowBox[{"yb", "-", "ya"}], "\[CapitalOmega]"], 
    RowBox[{"NIntegrate", "[", 
     RowBox[{
      RowBox[{"Exp", "[", 
       RowBox[{
        RowBox[{"-", "1"}], "/", 
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"u", "^", " ", "2"}]}], ")"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"u", ",", 
        RowBox[{"-", "1"}], ",", 
        FractionBox[
         RowBox[{
          RowBox[{"2", "x"}], "-", 
          RowBox[{"(", 
           RowBox[{"a", "+", "b"}], ")"}]}], 
         RowBox[{"b", "-", "a"}]]}], "}"}]}], "]"}]}], "+", "ya"}]}]], "Input",
 CellChangeTimes->{{3.537550039375*^9, 3.537550106234375*^9}}],

Cell[CellGroupData[{

