        RowBox[{"x", "<", "1"}]}], "}"}], ",", 
      RowBox[{"{", " ", 
       RowBox[{
        RowBox[{"fnum", "[", 
         RowBox[{"x", ",", "1", ",", 
          RowBox[{"3", "/", "2"}], ",", "2", ",", 
          RowBox[{"2", "/", "3"}]}], "]"}], ",", 
        RowBox[{"1", "<", "x"}]}], "}"}]}], "}"}], "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", "0", ",", 
     RowBox[{"3", "/", "2"}]}], "}"}], ",", 
